<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="270">
    <shape android:useLevel="false"
        android:shape="ring">

        <gradient
            android:startColor="#078E4D"
            android:endColor="#013E3B"
            android:centerColor="#078E4D"
            android:useLevel="false"
            android:type="linear"/>
        <stroke android:color="#078E4D"
            android:width="2dp"/>

    </shape>
</rotate>
